<%- include('../partials/header.ejs') -%>
<%- include('../partials/navbar.ejs') -%>
<div class="container">
  <section >
    <h2 class="pt-5 text-center" >Search Results</h2>

    <ul class="mb-5" style="list-style: none;">
  
      <% post.forEach(post => { %>
        <li class="mt-3">
          <a href="/post/<%= post._id %>" class="d-flex justify-content-between text-dark link-underline-light">
            <span><%= post.title %></span>
            <span class="article-list__date"><%= post.createdAt.toDateString() %></span>
          </a>
        </li>
        <hr>
      <% }) %>
    
     
    </ul>
  </section>
</div>


<%- include('../partials/footer.ejs') -%>